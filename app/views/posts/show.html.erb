<h1>Neogram</h1>

<article class="post">
  <%= image_tag  @post.image, class: @post.filter_class %>
  <% if @owner  %>
    <%= link_to  "Edit", edit_post_path(@post) %>
    <%= link_to  "Delete", post_path(@post), 
        method: :delete, 
        data: { confirm: 'Are you sure? '} %>
  <% end%>
</article>